{
  "name": "jupyterlab-hdf",
  "version": "0.1.0-alpha",
  "description": "Open and explore HDF5 files in Jupyterlab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/telamonian/jupyterlab-hdf5",
  "bugs": {
    "url": "https://github.com/telamonian/jupyterlab-hdf5/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Max Klein",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/telamonian/jupyterlab-hdf5.git"
  },
  "scripts": {
    "build": "jlpm install && tsc",
    "build:dev": "jlpm run build && jlpm run link",
    "build:watch": "jlpm run build --watch",
    "clean": "rimraf lib",
    "clean:slate": "jlpm run clean && rimraf node_modules",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "link:lab": "jupyter labextension link .",
    "unlink:lab": "jupyter labextension unlink jupyterlab-hdf5"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "@jupyterlab/codeeditor": "^0.19.1",
    "@jupyterlab/coreutils": "^2.2.1",
    "@jupyterlab/docmanager": "^0.19.1",
    "@jupyterlab/docregistry": "^0.19.1",
    "@jupyterlab/fileeditor": "^0.19.1",
    "@jupyterlab/services": "^3.2.1",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/datagrid": "^0.1.5",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/domutils": "^1.1.2",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.6.0",
    "react": "~16.4.2",
    "react-dom": "~16.4.2"
  },
  "devDependencies": {
    "@types/react": "~16.4.13",
    "@types/react-dom": "~16.0.7",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.5.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~3.1.0"
  },
  "resolutions": {
    "@types/react": "~16.4.13"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jupyterlab": {
    "extension": true
  }
}
